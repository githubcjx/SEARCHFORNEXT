.container {
  width: 100%;
  height: 100%;
  /* background-color: rgb(138, 235, 93); */
}

.header {
  width: 100%;
  height: 15%;
  padding: 30rpx 0 30rpx 0;
  background: linear-gradient(to bottom, rgb(139, 246, 90) 30%, #42cf48 100%);
}

.header .userInfo {
  width: 100%;
  display: flex;
  align-items: center;
  background-color: transparent;
}

.header .userInfo::after {
  border: none;
}

.header .userInfo .user_head {
  width: 130rpx;
  height: 130rpx;
  margin-left: 30rpx;
}

.header .userInfo .username {
  flex: 1;
  color: #fff;
  margin-left: 30rpx;
  font-size: 34rpx;
  letter-spacing: 5rpx;
  text-align: left;
}

.header .userInfo .enter_icon {
  width: 40rpx;
  height: 40rpx;
  margin-right: 40rpx;
}

.header open-data:nth-of-type(1) {
  width: 130rpx;
  height: 130rpx;
  margin-left: 60rpx;
  border-radius: 50%;
}

.header open-data:nth-of-type(2) {
  color: #fff;
  margin-left: 30rpx;
  font-size: 34rpx;
  letter-spacing: 5rpx;
  text-align: left;
}

.menu-list {
  margin-top: -50rpx;
}
.menu-list .main-menu {
  display: flex;
  justify-content: space-around;
}
.menu-list .main-menu .menu-item {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 26rpx;
  margin: 30rpx 20rpx;
  text-align: center;
}
.menu-list .main-menu .menu-item image {
  width: 80rpx;
  height: 80rpx;
}
.menu-list .main-menu .menu-item text {
  margin-top: 16rpx;
}

.nav {
  width: 90%;
  background-color: #fff;
  margin: 10rpx auto;
  border-radius: 10rpx;
  border: 1px solid #eee;
  box-shadow: 0 4rpx 8rpx #eee;
  z-index: 9;
}

.marginTop {
  margin-top: 40rpx;
}

.nav .listItem {
  width: 94%;
  height: 100rpx;
  font-size: 32rpx;
  line-height: 100rpx;
  background-color: #fff;
  margin: 0 auto;
}

.nav .listItem:not(:last-of-type) {
  border-bottom: 1px solid #eee;
}

.nav .listItem image {
  width: 40rpx;
  height: 40rpx;
  line-height: 60rpx;
  margin-right: 20rpx;
  vertical-align: middle;
}

.nav .listItem text::after {
  content: "â€º";
  color: #ddd;
  font-size: 60rpx;
  position: absolute;
  right: 60rpx;
}

.dialogText {
  width: 100%;
  height: 100%;
  padding: 80rpx 150rpx;
  box-sizing: border-box;
  text-align: center;
}

.logout {
  width: 100%;
}

.logout .exit {
  width: 60%;
  padding: 20rpx 0;
  color: #fff;
  background-color: #42cf48;
  margin: 0 auto;
  text-align: center;
  border-radius: 10rpx;
  margin-bottom: 40rpx;
}
