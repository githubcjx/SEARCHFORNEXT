.container {
  width: 100%;
  height: 100%;
  background-color: #eee;
}

.header {
  width: 100%;
  padding: 30rpx 0 30rpx 0;
  background: linear-gradient(to bottom, rgb(139, 246, 90) 30%, #42cf48 100%);
  opacity: 0.8;
}
.header .userInfo {
  width: 100%;
  display: flex;
  align-items: center;
  background-color: transparent;
}
.header .userInfo::after {
  border: none;
}
.header .userInfo .user_head {
  width: 130rpx;
  height: 130rpx;
  margin-left: 30rpx;
}
.header .userInfo .username {
  flex: 1;
  color: #fff;
  margin-left: 30rpx;
  font-size: 34rpx;
  letter-spacing: 5rpx;
  text-align: left;
}
.header .userInfo .enter_icon {
  width: 40rpx;
  height: 40rpx;
  margin-right: 40rpx;
}
.header open-data:nth-of-type(1) {
  width: 130rpx;
  height: 130rpx;
  margin-left: 60rpx;
}
.header open-data:nth-of-type(2) {
  color: #fff;
  margin-left: 30rpx;
  font-size: 34rpx;
  letter-spacing: 5rpx;
  text-align: left;
}

.nav {
  width: 100%;
  background-color: #fff;
  margin-top: 10rpx;
}

.marginTop {
  margin-top: 40rpx;
}

.nav .listItem {
  width: 94%;
  height: 100rpx;
  font-size: 32rpx;
  line-height: 100rpx;
  background-color: #fff;
  margin: 0 auto;
}

.nav .listItem:not(:last-of-type) {
  border-bottom: 1px solid #eee;
}

.nav .listItem image {
  width: 40rpx;
  height: 40rpx;
  line-height: 60rpx;
  margin-right: 20rpx;
  vertical-align: middle;
}

.nav .listItem text::after {
  content: "â€º";
  color: #ddd;
  font-size: 60rpx;
  position: absolute;
  right: 40rpx;
}

.dialogText {
  width: 100%;
  height: 100%;
  padding: 80rpx 150rpx;
  box-sizing: border-box;
  text-align: center;
}

.logout {
  width: 100%;
}
.logout .exit {
  width: 60%;
  padding: 20rpx 0;
  color: #fff;
  background-color: #42cf48;
  margin: 0 auto;
  text-align: center;
  border-radius: 10rpx;
  margin-bottom: 40rpx;
}